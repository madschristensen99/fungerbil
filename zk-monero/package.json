{
  "name": "zk-monero-payment",
  "version": "2.0.0",
  "description": "Zero-knowledge proof that a Monero payment happened - wallet-agnostic, zk-SNARK â†’ Solana/EVM",
  "main": "client/proveMoneroPayment.js",
  "scripts": {
    "build": "npm run build-circuits && npm run build-client",
    "build-circuits": "circom circuits/monero_final_working.circom --r1cs --wasm --sym -l node_modules/circomlib/circuits/",
    "build-client": "node client/proveMoneroPayment.js",
    "setup": "npm run build-circuits",
    "verify-sol": "solc onchain/solidity/MoneroPaymentVerifier.sol --bin --abi --optimize -o onchain/solidity/",
    "dev": "circom circuits/monero_final_working.circom --r1cs --wasm --sym",
    "test": "node test/run-test.js"
  },
  "keywords": [
    "monero",
    "zk-proof",
    "privacy",
    "defi",
    "solana",
    "evm",
    "ringct",
    "ed25519"
  ],
  "dependencies": {
    "@mymonero/mymonero-monero-client": "^3.0.3",
    "snarkjs": "^0.7.0",
    "axios": "^1.6.0",
    "circomlib": "^2.0.5"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "solc": "^0.8.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madschristensen99/fungerbil"
  },
  "license": "MIT"
}